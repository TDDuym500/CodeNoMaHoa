    country = "Philippines"
elseif string.find(locale, "my") then
    country = "Malaysia"
elseif string.find(locale, "us") then
    country = "Hoa K·ª≥"
elseif string.find(locale, "br") then
    country = "Brazil"
elseif string.find(locale, "kr") then
    country = "H√†n Qu·ªëc"
elseif string.find(locale, "jp") then
    country = "Nh·∫≠t B·∫£n"
